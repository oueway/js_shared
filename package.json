{
  "name": "@oueway/js-shared",
  "version": "1.0.4",
  "description": "Reusable authentication and dashboard components for Supabase-powered Next.js apps",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.mts",
      "import": "./dist/components/index.mjs"
    },
    "./lib": {
      "types": "./dist/lib/index.d.mts",
      "import": "./dist/lib/index.mjs"
    },
    "./pages": {
      "types": "./dist/pages/index.d.mts",
      "import": "./dist/pages/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepare": "pnpm build",
    "release": "pnpm version patch && git push && git push --tags"
  },
  "keywords": [
    "supabase",
    "authentication",
    "dashboard",
    "nextjs",
    "react"
  ],
  "peerDependencies": {
    "@supabase/ssr": ">=0.8.0",
    "lucide-react": ">=0.400.0",
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/react": "^18.2.0",
    "lucide-react": "^0.400.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
